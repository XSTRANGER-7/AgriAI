# AWS Configuration for AgriAI Platform
# Copy this file to .env and fill in your actual values

# AWS Region
VITE_AWS_REGION=us-east-1

# AWS Cognito Identity Pool ID (for unauthenticated access)
VITE_AWS_IDENTITY_POOL_ID=us-east-1:your-identity-pool-id

# Optional: AWS Access Keys (if not using Cognito Identity Pool)
# VITE_AWS_ACCESS_KEY_ID=your-access-key-id
# VITE_AWS_SECRET_ACCESS_KEY=your-secret-access-key

# SageMaker Endpoint Names (if you have custom endpoints)
VITE_SAGEMAKER_CROP_ENDPOINT=crop-recommendation-endpoint
VITE_SAGEMAKER_PEST_ENDPOINT=pest-detection-endpoint
VITE_SAGEMAKER_YIELD_ENDPOINT=yield-prediction-endpoint

# Bedrock Model IDs (default values, can be customized)
VITE_BEDROCK_CLAUDE_MODEL=anthropic.claude-3-sonnet-20240229-v1:0
VITE_BEDROCK_TITAN_MODEL=amazon.titan-text-express-v1